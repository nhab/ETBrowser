<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ETbrowser"
        xmlns:Components="clr-namespace:ETbrowser.Components" x:Class="ETbrowser.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>

        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>

        <TabControl Grid.Row="1">
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5">
                    <Components:ProjUserControl x:Name="projUC1"
                         HorizontalAlignment="Left" 
                             Margin="26,0,0,0" 
                             VerticalAlignment="Center" Height="117" Width="280"/>
                </Grid>
            </TabItem>
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
